<div class="text-center">
<h1>Conferences</h1>
</div>

<% @conference.each do |conference| %>
    <p>
        <%= image_tag conference.image_url %>
    </p>
    <p>
        <strong>Name:</strong>
        <%= conference.business_name %>
    </p>
    <p>
        <strong>Address:</strong>
        <%= conference.address %>
    </p>
    <p>
        <strong>About:</strong>
        <%= conference.about %>
    </p>
    <p>
        <%= link_to 'profile', caterer_information_path(conference.user_id) %>
    </p>

<% end %>

<%= link_to 'Back', event_types_path %>